{
  "name": "tanos.fm",
  "version": "1.5.1",
  "description": "A quick yet ad-free media downloader",
  "main": "index.js",
  "scripts": {
    "start": "npm run build --prefix client && node server/index.js",
    "server": "node server/index.js",
    "install:root": "npm ci",
    "install:client": "npm ci --prefix client",
    "install:server": "npm ci --prefix server",
    "install-deps": "npm run install:root && npm run install:client && npm run install:server",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "ascii-fullwidth-halfwidth-convert": "^0.1.1",
    "axios": "^1.8.3",
    "btch-downloader": "^4.1.21",
    "caniuse-lite": "^1.0.30001731",
    "cheerio": "^1.0.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "ip-range-check": "^0.2.0",
    "ipify": "^6.0.0",
    "lrclib-api": "^2.0.0",
    "node-fetch": "^3.3.2",
    "node-id3": "^0.2.8",
    "ruhend-scraper": "^9.0.1",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.33.5",
    "soundcloud-downloader": "^1.0.0",
    "soundcloud-key-fetch": "^1.0.13",
    "tinyurl": "^1.1.7",
    "twdl": "^2.11.1",
    "twitter-dl": "^1.0.6",
    "twitter-downloader": "^1.1.8",
    "vidzit-dl": "^0.0.2",
    "ytdl-core": "^4.11.5",
    "ytmp3-js": "^1.1.2"
  }
}
